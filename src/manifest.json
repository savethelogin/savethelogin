{
  "name": "Save the Login",
  "description": "",
  "default_locale": "en",
  "version": "1.0.5",
  "permissions": [
    "tabs",
    "activeTab",
    "cookies",
    "storage",
    "downloads",
    "management",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "<all_urls>"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png",
    "default_title": "Save the login"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
