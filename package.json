    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^1.1.11",
    "flush-promises": "^1.0.2",
    "husky": "^2.7.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.3.0",
    "karma-browserify": "^6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
